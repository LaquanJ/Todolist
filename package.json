{
  "name": "web-todos",
  "private": true,
  "version": "0.0.1",
  "description": "A template for a ReactJS SPA frontend website.",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test:unit": "JEST_JUNIT_OUTPUT_NAME=unit.xml craco test --watchAll=false --coverage unit",
    "test:integration": "JEST_JUNIT_OUTPUT_NAME=integration.xml craco test --watchAll=false integration",
    "test-debug": "craco --inspect-brk=0.0.0.0:9230 test --watchAll=false --runInBand --no-cache --reporters=default",
    "test-lint": "npx eslint -c default.eslintrc.json --ext .jsx,.js  src/"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "axios": "1.4.0",
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.6.1",
    "buffer": "6.0.3",
    "crypto": "npm:crypto-browserify@3.12.0",
    "jwt-decode": "3.1.2",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-md": "5.1.5",
    "react-scripts": ">=4.0.3",
    "react-router-dom": "5.3.0",
    "react-bootstrap": "2.0.0",
    "react-router-bootstrap": "0.25.0",
    "react-toastify": "8.0.3",
    "stream": "npm:stream-browserify@3.0.0",
    "util": "0.12.5"
  },
  "overrides": {
    "node-forge": "^1.3.1",
    "immer": "^9.0.15",
    "ejs": "^3.1.8",
    "shell-quote": "^1.7.3",
    "@hapi/hoek": "10.0.1",
    "unset-value": "2.0.1",
    "minimatch": ">=9.0.3",
    "nth-check": ">=2.0.1",
    "css-what": ">=5.0.1",
    "glob": "^9.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "chokidar": "^3.5.1",
    "prettier": "^2.3.1",
    "jest": "^29.6.1",
    "jest-junit": "^16.0.0",
    "react-error-overlay": "6.0.9",
    "sass": "1.37.5",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "7.32.2"
  }
}